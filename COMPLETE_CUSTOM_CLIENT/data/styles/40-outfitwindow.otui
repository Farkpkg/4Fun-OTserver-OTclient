      margin-top: 1
      change-cursor-image: false
      @onCheckChange: modules.game_outfit.onOnlyMineThings()
      $disabled:
        color: #707070
      $hover !disabled:
        color: $var-text-cip-color

    TextEdit
      id: searchfilter
      size: 137 22
      image-source: /images/ui/search-text-void
      anchors.left: onlyCheck.right
      anchors.verticalCenter: parent.verticalCenter
      margin-left: 12
      margin-top: -1
      text-align: LeftCenter
      !placeholder: tr("Type to search")
      placeholder-color: #707070
      placeholder-align: center
      placeholder-align: left
      focusable: true
      padding-left: 4
      padding-top: 2
      box-text-padding: 28
      @onTextChange: modules.game_outfit.onFilterSearch(self)
      UIButton
        id: clearButton
        size: 20 20
        anchors.verticalCenter: parent.verticalCenter
        anchors.right: parent.right
        image-source: /images/ui/erase-button
        image-clip: 0 0 20 20
        margin-top: -1
        margin-right: 1
        padding: 1
        @onClick: modules.game_outfit.onClearFilterSearch(self:getParent())

        $hover !disabled:
          image-clip: 0 0 20 20

        $pressed:
          image-clip: 0 20 20 20

        $on:
          image-clip: 0 20 20 20

        $checked:
          image-clip: 0 20 20 20

        $disabled:
          image-clip: 0 0 20 20
