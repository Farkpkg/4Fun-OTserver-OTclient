FloorTile < UIWidget
  image-source: /images/game/floor
  size: 64 64
  phantom: true

MiniPanel < Panel
  text-offset: 0 3
  text-align: top
  image-source: /images/ui/minipanel
  image-border: 2
  image-border-top: 19
  padding-left: 7
  padding-bottom: 7
  padding-top: 24
  padding-right: 7
  color: #929292

RotateButton < UIButton
  anchors.bottom: parent.bottom
  size: 20 20
  margin-bottom: 3
  image-source: /images/ui/rotate_button
  image-color: #dfdfdf

  $hover:
    image-color: #ffffff

PresetButton < UIButton
  size: 218 100
  focusable: true
  image-source: /images/ui/big-light-button
  image-clip: 0 0 218 100

  UIWidget
    id: info
    size: 12 12
    anchors.right: parent.right
    anchors.top: parent.top
    image-source: /images/ui/info
    margin-top: 5
    margin-right: 5
    visible: false
    tooltip: Note that you won't be able to use the parts of this preset that you\ndo not own.
  
  UICreature
    id: outfit
    size: 64 64
    anchors.horizontalCenter: parent.horizontalCenter
    anchors.verticalCenter: parent.verticalCenter
    margin-bottom: 13
    idle-animate: true
    phantom: true

  UICreature
    id: familiar
    size: 64 64
    anchors.horizontalCenter: parent.horizontalCenter
    anchors.verticalCenter: parent.verticalCenter
    margin-bottom: 13
    margin-left: 32
    idle-animate: true
    phantom: true
    visible: false

  Label
    id: name
    anchors.horizontalCenter: parent.horizontalCenter
    anchors.verticalCenter: parent.verticalCenter
    margin-top: 39
    text-auto-resize: true
    color: $var-text-cip-color
    font: $var-cip-font

SelectionButton < UIButton
  size: 108 100
  image-source: /images/ui/large_gray_button
  image-clip: 0 0 108 100
  focusable: true

  $pressed:
    image-clip: 0 100 108 100 

  $focus:
    border: 1 white

  $!focus:
    border: 0 alpha

  UICreature
    id: outfit
    size: 64 64
    anchors.verticalCenter: parent.verticalCenter
    anchors.horizontalCenter: parent.horizontalCenter
    margin-bottom: 13
    idle-animate: true
  
  Label
    id: name
    anchors.bottom: parent.bottom
    anchors.left: parent.left
    anchors.right: parent.right
    margin-left: 5
    margin-right: 5
    margin-bottom: 4
    text-align: bottom
    text-wrap: true
    text-auto-resize: true
    color: $var-text-cip-color
    font: $var-cip-font

