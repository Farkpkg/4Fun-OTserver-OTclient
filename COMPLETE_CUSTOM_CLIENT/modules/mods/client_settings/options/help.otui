HelpWindow < Panel
  id: help
  anchors.fill: parent
  text-wrap: true
  image-source: /images/ui/2pixel-up-frame-borderimage
  image-border: 3
  padding: 3
  enabled: false

  Panel
    anchors.fill: parent
    margin: 5 5 5 3

    FlatPanel
      size: 505 180
      anchors.top: parent.top
      anchors.left: parent.left

      Button
        id: clientHelpButton
        size: 128 20
        !text: tr('Client Help')
        anchors.top: parent.top
        anchors.left: parent.left
        margin-top: 5
        margin-left: 5

      UIWidget
        size: 12 12
        anchors.top: prev.top
        anchors.right: parent.right
        margin-right: 5
        margin-top: 5
        image-source: /images/skin/show-gui-help-grey
        !tooltip: tr('toDo')

      Button
        id: tutorHelpButton
        size: 128 20
        !text: tr('Tutor Help')
        anchors.top: clientHelpButton.bottom
        anchors.left: parent.left
        margin-top: 6
        margin-left: 5

      UIWidget
        size: 12 12
        anchors.top: prev.top
        anchors.right: parent.right
        margin-right: 5
        margin-top: 5
        image-source: /images/skin/show-gui-help-grey
        !tooltip: tr('toDo')

      Button
