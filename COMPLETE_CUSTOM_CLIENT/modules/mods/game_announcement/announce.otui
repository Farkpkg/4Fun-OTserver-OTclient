AnnounceWindow < NewHeadlessWindow
  id: announceWindow
  anchors.horizontalCenter: parent.horizontalCenter
  size: 620 180
  @onEscape: close()
  TextList
    id: announceList
    anchors.top: parent.top
    anchors.left: parent.left
    anchors.right: parent.right
    anchors.bottom: separator.top
    margin: 12 15 12 15
    padding: 1
    focusable: true
    padding-top: 2
    padding-left: 5
    padding-right: 10
    vertical-scrollbar: announceScrollBar
    font: $var-cip-font
    color: $var-text-cip-color-highlight
    text-auto-resize: true
    text-wrap: true
    editable: false
    multi-line: true
  VerticalScrollBar
    id: announceScrollBar
    anchors.top: prev.top
    anchors.right: prev.right
    anchors.bottom: prev.bottom
    margin: 1 1 1 0
    step: 25
    pixels-scroll: true
    visible: false
  HorizontalSeparator
    id: separator
    anchors.bottom: closeButton.top
    anchors.left: parent.left
    anchors.right: parent.right
    margin-bottom: 9
    margin-left: 15
    margin-right: 15
  Button
    id: closeButton
    anchors.bottom: parent.bottom
    anchors.horizontalCenter: parent.horizontalCenter
    !text: tr('Close')
    size: 43 20
    margin-bottom: 10
    @onClick: close()
  Button
    id: nextButton
    size: 20 20
    anchors.top: prev.top
    anchors.left: prev.right
    margin-left: 5
    image-source: /images/game/highscore/arrow
    image-clip: 20 20 20 20
    visible: false
    $pressed:
      image-clip: 60 20 20 20
    $hover !pressed:
      image-clip: 20 20 20 20
  Button
    id: openLinkButton
    size: 53 20
    anchors.bottom: parent.bottom
    anchors.right: parent.right
    !text: tr('Open Link')
    image-source: /images/ui/buttons-blue
    margin-bottom: 10
    margin-right: 15
    visible: false
    @onClick: openRedirectLink()
  UIWidget
    id: borderImage
    size: 55 22
    anchors.top: prev.top
    anchors.left: prev.left
    margin-top: -1
    margin-left: -1
    image-source: /images/store/rectangle-highlight
    image-border: 1
    phantom: true
    visible: false
  UIWidget
    id: attentionBanner
    size: 520 98
    anchors.top: parent.top
    anchors.horizontalCenter: parent.horizontalCenter
    margin-top: -85
    margin-left: 3
    image-source: /images/ui/notification/attention_banner
    phantom: true

