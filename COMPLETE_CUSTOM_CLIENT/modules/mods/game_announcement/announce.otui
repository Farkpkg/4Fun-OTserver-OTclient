SetupPool < NewHeadlessWindow
  id: setuPoolWindow
  anchors.horizontalCenter: parent.horizontalCenter
  size: 430 330
  @onEscape:
  UIWidget
    size: 145 56
    anchors.top: parent.top
    anchors.horizontalCenter: parent.horizontalCenter
    margin-top: -33
    image-source: /images/game/tutorial/backdrop_crestr
    phantom: true
  Panel
    id: contentPanel
    anchors.fill: parent
    margin: 15 13 17 13
    Panel
      id: setupPoolPanel
      height: 60
      anchors.top: parent.top
      anchors.left: parent.left
      anchors.right: parent.right
      anchors.bottom: separator.top
      margin-top: 11
      margin-bottom: 10
      image-source: /images/ui/t2pixel-up-frame-borderimage
      image-border: 35
      Label
        id: title
        anchors.top: parent.top
        anchors.horizontalCenter: parent.horizontalCenter
        margin-top: 2
        !text: tr('Setup Answer Option')
        font: $var-cip-font
        color: $var-text-cip-color-grey
        text-auto-resize: true
      Label
        id: setupOptionLabel
        anchors.top: prev.bottom
        anchors.left: parent.left
        margin-top: 5
        margin-left: 8
        !text: tr('Insert a answer to the player:')
        text-wrap: true
        font: $var-cip-font
        color: $var-text-cip-color
        text-auto-resize: true
      TextEdit
        id: setupOptionList
        height: 20
        anchors.top: prev.bottom
        anchors.left: parent.left
        anchors.right: parent.right
        margin: 8 8 0 8
        focusable: true
        padding-top: 2
        padding-left: 5
        padding-right: 15
        @onTextChange: modules.game_announcement.onEditOption()
      Label
        id: setupDescLabel
        anchors.top: prev.bottom
        anchors.left: parent.left
        margin-top: 5
        margin-left: 8
        !text: tr('Insert a description to this answer:')
        text-wrap: true
        font: $var-cip-font
        color: $var-text-cip-color
        text-auto-resize: true
      TextEdit
        id: setupDescriptionList
        height: 150
        anchors.top: prev.bottom
        anchors.left: parent.left
        anchors.right: parent.right
        margin: 8 8 0 8
        focusable: true
        padding-top: 2
        padding-left: 5
        padding-right: 15
        multiline: true
        text-wrap: true
        @onTextChange: modules.game_announcement.onEditOption()
    HorizontalSeparator
      id: separator
      anchors.bottom: closeButton.top
      anchors.left: parent.left
      anchors.right: parent.right
      margin-bottom: 9
    Button
      id: closeButton
      anchors.bottom: parent.bottom
      anchors.right: parent.right
      !text: tr('Close')
      size: 43 20
      @onClick: closePollAnnouncement()
    Button
      id: insertButton
      size: 43 20
      !text: tr('Insert')
      anchors.bottom: parent.bottom
      anchors.right: prev.left
      margin-right: 5
      enabled: false
      @onClick:

