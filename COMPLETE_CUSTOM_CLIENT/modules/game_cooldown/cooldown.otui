    image-clip: 66 0 22 24
    anchors.top: parent.top
    anchors.left: groupIconSupport.right
    margin-left: 4
    tooltip: Special
    tooltip-delayed: true
    $on:
      image-clip: 66 24 22 24

  BarSpellProgressRect
