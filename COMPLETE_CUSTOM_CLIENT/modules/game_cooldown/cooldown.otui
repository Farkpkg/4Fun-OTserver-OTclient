BarSpellGroupIcon < UIWidget
  size: 22 24
  image-size: 22 24
  image-source: /images/game/spells/cooldowns
  focusable: false
  margin-top: 2

BarSpellIcon < UIWidget
  size: 22 24
  image-size: 22 24
  focusable: false
  tooltip-delayed: true

  $!first:
    margin-left: 1

BarSpellProgressRect < ProgressBarSD
  size: 20 2
  percent: 0
  image-source: /images/game/spells/progress
  image-clip: 0 0 20 2

GroupCooldownParticles < UIParticles
  effect: groupcooldown-effect

Panel
  id: panelCooldown
  height: 26
  focusable: false
  image-source: /images/ui/background-dark
  image-border: 4
  BarSpellGroupIcon
    id: groupIconAttack
    image-clip: 0 0 22 24
    anchors.top: parent.top
    anchors.left: parent.left
    margin-left: 2
    tooltip: Attack
    tooltip-delayed: true
    $on:
      image-clip: 0 24 22 24

  BarSpellProgressRect
    id: progressRectAttack
    anchors.bottom: groupIconAttack.bottom
    anchors.left: groupIconAttack.left
    margin-bottom: 1
    margin-left: 1
    !tooltip: tr('Attack')

  BarSpellGroupIcon
    id: groupIconHealing
    image-clip: 22 0 22 24
    anchors.top: parent.top
    anchors.left: groupIconAttack.right
    margin-left: 4
    tooltip: Healing
    tooltip-delayed: true
    $on:
      image-clip: 22 24 22 24

  BarSpellProgressRect
    id: progressRectHealing
    anchors.bottom: groupIconHealing.bottom
    anchors.left: groupIconHealing.left
    margin-bottom: 1
    margin-left: 1
    !tooltip: tr('Healing')

  BarSpellGroupIcon
    id: groupIconSupport
    image-clip: 44 0 22 24
    anchors.top: parent.top
    anchors.left: groupIconHealing.right
    margin-left: 4
    tooltip: Support
    tooltip-delayed: true
