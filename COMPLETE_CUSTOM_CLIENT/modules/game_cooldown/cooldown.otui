  VerticalSeparator
    id: separator
    anchors.top: parent.top
    anchors.left: groupIconVirtue.right
    margin-left: 5
    margin-top: 2
    height: 22

  FlatPanel
    id: cooldownPanel
    image-source: /images/ui/background-dark
    layout:
      type: horizontalBox
    anchors.top: parent.top
    anchors.left: separator.right
    margin-left: 5
    margin-top: 2
    height: 22