    anchors.top: parent.top
    anchors.left: groupIconSpecial.right
    margin-left: 4
    tooltip: Cripple
    tooltip-delayed: true
    $on:
      image-clip: 88 24 22 24

  BarSpellProgressRect
    id: progressRectCripple
